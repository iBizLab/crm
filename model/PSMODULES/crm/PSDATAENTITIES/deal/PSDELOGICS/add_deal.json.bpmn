<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
    <definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:activiti="http://activiti.org/bpmn" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:g="http://www.jboss.org/drools/flow/gpd" xmlns:tns="http://www.jboss.org/drools" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" expressionLanguage="http://www.mvel.org/2.0" id="Definition" name="" targetNamespace="http://www.jboss.org/drools" typeLanguage="http://www.java.com/javaTypes">
        <process id="cn.ibizlab.crm.core.crm.service.logic.dealadd_deal" isClosed="false" isExecutable="true" name="crm_deal_add_deal_ruleFlow" processType="Private" tns:packageName="cn.ibizlab.crm.core.crm.service.logic.dealadd_deal">
            <extensionElements>
                <tns:import name="java.util.Map" />
                <tns:import name="org.springframework.util.StringUtils"/>
                <tns:import name="cn.ibizlab.crm.util.helper.RuleUtils"/>
                                <tns:metaData express="" name="dealadd_dealdefault" type="entity"/>
                                <tns:metaData express="new cn.ibizlab.crm.core.crm.domain.deal()" name="dealadd_dealitem" type="refentity"/>
                                <tns:metaData express="new cn.ibizlab.crm.core.crm.domain.relation()" name="dealadd_dealrelation" type="refentity"/>
                                <tns:metaData express="new HashMap()" name="dealadd_dealsrfactionparam" type="refentity"/>
                                <tns:metaData express="new HashMap()" name="dealadd_dealsub" type="refentity"/>
                                    <tns:metaData express="T(cn.ibizlab.crm.util.security.SpringContextHolder).getBean(T(cn.ibizlab.crm.core.crm.service.IrelationService))" name="relationservice" type="service"/>
                    <tns:metaData express="T(cn.ibizlab.crm.util.security.SpringContextHolder).getBean(T(cn.ibizlab.crm.core.crm.service.IdealService))" name="iBzSysDealDefaultService" type="service"/>
                    <tns:metaData express="T(cn.ibizlab.crm.util.security.AuthenticationUser).getAuthenticationUser()" name="curuser" type="session"/>
                            <tns:global identifier="dealadd_dealdefault" type="cn.ibizlab.crm.core.crm.domain.deal" />
                            <tns:global identifier="dealadd_dealitem" type="cn.ibizlab.crm.core.crm.domain.deal" />
                            <tns:global identifier="dealadd_dealrelation" type="cn.ibizlab.crm.core.crm.domain.relation" />
            </extensionElements>
                        <startEvent id="begin" isInterrupting="true"/>
                        <businessRuleTask activiti:exclusive="true" g:ruleFlowGroup="dealadd_dealprepareparam1" id="prepareparam1" implementation="http://www.jboss.org/drools/rule" name="获取选择商机集合"/>
                        <businessRuleTask activiti:exclusive="true" g:ruleFlowGroup="dealadd_dealloopsubcall1" id="loopsubcall1" implementation="http://www.jboss.org/drools/rule" name="循环获取商机数据集"/>
                        <businessRuleTask activiti:exclusive="true" g:ruleFlowGroup="dealadd_dealcopyparam1" id="copyparam1" implementation="http://www.jboss.org/drools/rule" name="拷贝商机数据至中间对象"/>
                        <businessRuleTask activiti:exclusive="true" g:ruleFlowGroup="dealadd_dealend1" id="end1" implementation="http://www.jboss.org/drools/rule" name="结束"/>
                        <endEvent id="end1_end" name="end"/>
                        <sequenceFlow id="end1_end_line" sourceRef="end1" targetRef="end1_end"/>
                        <businessRuleTask activiti:exclusive="true" g:ruleFlowGroup="dealadd_dealprepareparam2" id="prepareparam2" implementation="http://www.jboss.org/drools/rule" name="设置关联数据"/>
                        <businessRuleTask activiti:exclusive="true" g:ruleFlowGroup="dealadd_dealdeaction1" id="deaction1" implementation="http://www.jboss.org/drools/rule" name="创建关联数据"/>
                        <endEvent id="deaction1_end" name="end"/>
                        <sequenceFlow id="deaction1_end_line" sourceRef="deaction1" targetRef="deaction1_end"/>
                    <sequenceFlow id="gateway-begin_prepareparam1" sourceRef="gateway-begin" targetRef="prepareparam1">
                            <conditionExpression  language="http://www.jboss.org/drools/rule" ><![CDATA[
                                            $dealadd_dealdefault:deal();
                                            $dealadd_dealitem:deal();
                                            $dealadd_dealrelation:relation();
                                            $dealadd_dealsrfactionparam:Map();
                                            $dealadd_dealsub:Map();
                                    eval($dealadd_dealdefault==dealadd_dealdefault && $dealadd_dealitem==dealadd_dealitem && $dealadd_dealrelation==dealadd_dealrelation && );
                                eval((RuleUtils.test($dealadd_dealdefault.get("srfactionparam"),"ISNOTNULL","")))
                                ]]>
                            </conditionExpression>
                    </sequenceFlow>
                    <sequenceFlow id="prepareparam1_loopsubcall1" sourceRef="prepareparam1" targetRef="loopsubcall1">
                    </sequenceFlow>
                    <sequenceFlow id="prepareparam1_end1" sourceRef="prepareparam1" targetRef="end1">
                    </sequenceFlow>
                    <sequenceFlow id="loopsubcall1_copyparam1" sourceRef="loopsubcall1" targetRef="copyparam1">
                    </sequenceFlow>
                    <sequenceFlow id="copyparam1_prepareparam2" sourceRef="copyparam1" targetRef="prepareparam2">
                    </sequenceFlow>
                    <sequenceFlow id="prepareparam2_deaction1" sourceRef="prepareparam2" targetRef="deaction1">
                    </sequenceFlow>
                            <exclusiveGateway id="gateway-begin" name="Gateway" gatewayDirection="Diverging" default="gateway-begin_end_line"></exclusiveGateway>
                            <sequenceFlow id="begin_gateway-begin_gatewayline" sourceRef="begin" targetRef="gateway-begin"></sequenceFlow>
                            <sequenceFlow id="gateway-begin_end_line" sourceRef="gateway-begin" targetRef="gateway-begin_end"/>
                            <endEvent id="gateway-begin_end" name="end"/>
        </process>
    </definitions>