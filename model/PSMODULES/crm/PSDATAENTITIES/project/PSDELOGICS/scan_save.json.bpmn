<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
    <definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:activiti="http://activiti.org/bpmn" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:g="http://www.jboss.org/drools/flow/gpd" xmlns:tns="http://www.jboss.org/drools" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" expressionLanguage="http://www.mvel.org/2.0" id="Definition" name="" targetNamespace="http://www.jboss.org/drools" typeLanguage="http://www.java.com/javaTypes">
        <process id="cn.ibizlab.crm.core.crm.service.logic.projectscan_save" isClosed="false" isExecutable="true" name="crm_project_scan_save_ruleFlow" processType="Private" tns:packageName="cn.ibizlab.crm.core.crm.service.logic.projectscan_save">
            <extensionElements>
                <tns:import name="java.util.Map" />
                <tns:import name="org.springframework.util.StringUtils"/>
                <tns:import name="cn.ibizlab.crm.util.helper.RuleUtils"/>
                                <tns:metaData express="" name="projectscan_savedefault" type="entity"/>
                                <tns:metaData express="new cn.ibizlab.crm.core.crm.domain.project()" name="projectscan_saveproject_copy" type="refentity"/>
                                    <tns:metaData express="T(cn.ibizlab.crm.util.security.SpringContextHolder).getBean(T(cn.ibizlab.crm.core.crm.service.IprojectService))" name="projectservice" type="service"/>
                                    <tns:metaData express="T(cn.ibizlab.crm.util.security.SpringContextHolder).getBean(T(cn.ibizlab.crm.core.crm.service.Ipayee_statisticService))" name="payee_statisticservice" type="service"/>
                    <tns:metaData express="T(cn.ibizlab.crm.util.security.SpringContextHolder).getBean(T(cn.ibizlab.crm.core.crm.service.IprojectService))" name="iBzSysProjectDefaultService" type="service"/>
                    <tns:metaData express="T(cn.ibizlab.crm.util.security.AuthenticationUser).getAuthenticationUser()" name="curuser" type="session"/>
                            <tns:global identifier="projectscan_savedefault" type="cn.ibizlab.crm.core.crm.domain.project" />
                            <tns:global identifier="projectscan_saveproject_copy" type="cn.ibizlab.crm.core.crm.domain.project" />
            </extensionElements>
                        <startEvent id="begin" isInterrupting="true"/>
                        <businessRuleTask activiti:exclusive="true" g:ruleFlowGroup="projectscan_saveprepareparam1" id="prepareparam1" implementation="http://www.jboss.org/drools/rule" name="填充当前订单的副本"/>
                        <businessRuleTask activiti:exclusive="true" g:ruleFlowGroup="projectscan_savedelogic1" id="delogic1" implementation="http://www.jboss.org/drools/rule" name="循环子订单判断"/>
                        <businessRuleTask activiti:exclusive="true" g:ruleFlowGroup="projectscan_saverawsfcode1" id="rawsfcode1" implementation="http://www.jboss.org/drools/rule" name="设置关闭时间"/>
                        <businessRuleTask activiti:exclusive="true" g:ruleFlowGroup="projectscan_savedeaction1" id="deaction1" implementation="http://www.jboss.org/drools/rule" name="保存订单信息"/>
                        <businessRuleTask activiti:exclusive="true" g:ruleFlowGroup="projectscan_savedeaction2" id="deaction2" implementation="http://www.jboss.org/drools/rule" name="重新生成统计数据"/>
                        <businessRuleTask activiti:exclusive="true" g:ruleFlowGroup="projectscan_saveend1" id="end1" implementation="http://www.jboss.org/drools/rule" name="结束"/>
                        <endEvent id="end1_end" name="end"/>
                        <sequenceFlow id="end1_end_line" sourceRef="end1" targetRef="end1_end"/>
                    <sequenceFlow id="begin_prepareparam1" sourceRef="begin" targetRef="prepareparam1">
                    </sequenceFlow>
                    <sequenceFlow id="gateway-prepareparam1_delogic1" sourceRef="gateway-prepareparam1" targetRef="delogic1">
                            <conditionExpression  language="http://www.jboss.org/drools/rule" ><![CDATA[
                                            $projectscan_savedefault:project();
                                            $projectscan_saveproject_copy:project();
                                    eval($projectscan_savedefault==projectscan_savedefault && $projectscan_saveproject_copy==projectscan_saveproject_copy);
                                eval((RuleUtils.test($projectscan_savedefault.get("status"),"EQ","20")))
                                ]]>
                            </conditionExpression>
                    </sequenceFlow>
                    <sequenceFlow id="gateway-prepareparam1_deaction1" sourceRef="gateway-prepareparam1" targetRef="deaction1">
                            <conditionExpression  language="http://www.jboss.org/drools/rule" ><![CDATA[
                                            $projectscan_savedefault:project();
                                            $projectscan_saveproject_copy:project();
                                    eval($projectscan_savedefault==projectscan_savedefault && $projectscan_saveproject_copy==projectscan_saveproject_copy);
                                eval((RuleUtils.test($projectscan_savedefault.get("status"),"NOTEQ","20")))
                                ]]>
                            </conditionExpression>
                    </sequenceFlow>
                    <sequenceFlow id="delogic1_rawsfcode1" sourceRef="delogic1" targetRef="rawsfcode1">
                    </sequenceFlow>
                    <sequenceFlow id="rawsfcode1_deaction1" sourceRef="rawsfcode1" targetRef="deaction1">
                    </sequenceFlow>
                    <sequenceFlow id="deaction1_deaction2" sourceRef="deaction1" targetRef="deaction2">
                    </sequenceFlow>
                    <sequenceFlow id="deaction2_end1" sourceRef="deaction2" targetRef="end1">
                    </sequenceFlow>
                            <exclusiveGateway id="gateway-prepareparam1" name="Gateway" gatewayDirection="Diverging" default="gateway-prepareparam1_end_line"></exclusiveGateway>
                            <sequenceFlow id="prepareparam1_gateway-prepareparam1_gatewayline" sourceRef="prepareparam1" targetRef="gateway-prepareparam1"></sequenceFlow>
                            <sequenceFlow id="gateway-prepareparam1_end_line" sourceRef="gateway-prepareparam1" targetRef="gateway-prepareparam1_end"/>
