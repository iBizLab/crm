<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
    <definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:activiti="http://activiti.org/bpmn" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:g="http://www.jboss.org/drools/flow/gpd" xmlns:tns="http://www.jboss.org/drools" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" expressionLanguage="http://www.mvel.org/2.0" id="Definition" name="" targetNamespace="http://www.jboss.org/drools" typeLanguage="http://www.java.com/javaTypes">
        <process id="cn.ibizlab.crm.core.crm.service.logic.tasknotify" isClosed="false" isExecutable="true" name="crm_task_notify_ruleFlow" processType="Private" tns:packageName="cn.ibizlab.crm.core.crm.service.logic.tasknotify">
            <extensionElements>
                <tns:import name="java.util.Map" />
                <tns:import name="org.springframework.util.StringUtils"/>
                <tns:import name="cn.ibizlab.crm.util.helper.RuleUtils"/>
                                <tns:metaData express="" name="tasknotifydefault" type="entity"/>
                                    <tns:metaData express="T(cn.ibizlab.crm.util.security.SpringContextHolder).getBean(T(cn.ibizlab.crm.core.crm.service.ItaskService))" name="taskservice" type="service"/>
                    <tns:metaData express="T(cn.ibizlab.crm.util.security.SpringContextHolder).getBean(T(cn.ibizlab.crm.core.crm.service.ItaskService))" name="iBzSysTaskDefaultService" type="service"/>
                    <tns:metaData express="T(cn.ibizlab.crm.util.security.AuthenticationUser).getAuthenticationUser()" name="curuser" type="session"/>
                            <tns:global identifier="tasknotifydefault" type="cn.ibizlab.crm.core.crm.domain.task" />
            </extensionElements>
                        <startEvent id="begin" isInterrupting="true"/>
                        <businessRuleTask activiti:exclusive="true" g:ruleFlowGroup="tasknotifydenotify1" id="denotify1" implementation="http://www.jboss.org/drools/rule" name="推送任务新建、更新通知"/>
                        <endEvent id="denotify1_end" name="end"/>
                        <sequenceFlow id="denotify1_end_line" sourceRef="denotify1" targetRef="denotify1_end"/>
                        <businessRuleTask activiti:exclusive="true" g:ruleFlowGroup="tasknotifydenotify2" id="denotify2" implementation="http://www.jboss.org/drools/rule" name="推送活动新建、更新通知"/>
                        <endEvent id="denotify2_end" name="end"/>
                        <sequenceFlow id="denotify2_end_line" sourceRef="denotify2" targetRef="denotify2_end"/>
                    <sequenceFlow id="gateway-begin_denotify1" sourceRef="gateway-begin" targetRef="denotify1">
                            <conditionExpression  language="http://www.jboss.org/drools/rule" ><![CDATA[
                                            $tasknotifydefault:task();
                                    eval($tasknotifydefault==tasknotifydefault);
                                eval((RuleUtils.test($tasknotifydefault.get("type"),"EQ","TASK")))
                                ]]>
                            </conditionExpression>
                    </sequenceFlow>
                    <sequenceFlow id="gateway-begin_denotify2" sourceRef="gateway-begin" targetRef="denotify2">
                            <conditionExpression  language="http://www.jboss.org/drools/rule" ><![CDATA[
                                            $tasknotifydefault:task();
                                    eval($tasknotifydefault==tasknotifydefault);
                                eval((RuleUtils.test($tasknotifydefault.get("type"),"EQ","MANEUVER")))
                                ]]>
                            </conditionExpression>
                    </sequenceFlow>
                            <exclusiveGateway id="gateway-begin" name="Gateway" gatewayDirection="Diverging" default="gateway-begin_end_line"></exclusiveGateway>
                            <sequenceFlow id="begin_gateway-begin_gatewayline" sourceRef="begin" targetRef="gateway-begin"></sequenceFlow>
                            <sequenceFlow id="gateway-begin_end_line" sourceRef="gateway-begin" targetRef="gateway-begin_end"/>
                            <endEvent id="gateway-begin_end" name="end"/>
        </process>
    </definitions>