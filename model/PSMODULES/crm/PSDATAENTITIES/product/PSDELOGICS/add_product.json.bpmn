<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
    <definitions xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:activiti="http://activiti.org/bpmn" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:g="http://www.jboss.org/drools/flow/gpd" xmlns:tns="http://www.jboss.org/drools" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" expressionLanguage="http://www.mvel.org/2.0" id="Definition" name="" targetNamespace="http://www.jboss.org/drools" typeLanguage="http://www.java.com/javaTypes">
        <process id="cn.ibizlab.crm.core.crm.service.logic.productadd_product" isClosed="false" isExecutable="true" name="crm_product_add_product_ruleFlow" processType="Private" tns:packageName="cn.ibizlab.crm.core.crm.service.logic.productadd_product">
            <extensionElements>
                <tns:import name="java.util.Map" />
                <tns:import name="org.springframework.util.StringUtils"/>
                <tns:import name="cn.ibizlab.crm.util.helper.RuleUtils"/>
                                <tns:metaData express="" name="productadd_productdefault" type="entity"/>
                                <tns:metaData express="new cn.ibizlab.crm.core.crm.domain.product()" name="productadd_productitem" type="refentity"/>
                                <tns:metaData express="new cn.ibizlab.crm.core.crm.domain.relation()" name="productadd_productrelation" type="refentity"/>
                                <tns:metaData express="new HashMap()" name="productadd_productsrfactionparam" type="refentity"/>
                                <tns:metaData express="new HashMap()" name="productadd_productsub" type="refentity"/>
                                    <tns:metaData express="T(cn.ibizlab.crm.util.security.SpringContextHolder).getBean(T(cn.ibizlab.crm.core.crm.service.IrelationService))" name="relationservice" type="service"/>
                    <tns:metaData express="T(cn.ibizlab.crm.util.security.SpringContextHolder).getBean(T(cn.ibizlab.crm.core.crm.service.IproductService))" name="iBzSysProductDefaultService" type="service"/>
                    <tns:metaData express="T(cn.ibizlab.crm.util.security.AuthenticationUser).getAuthenticationUser()" name="curuser" type="session"/>
                            <tns:global identifier="productadd_productdefault" type="cn.ibizlab.crm.core.crm.domain.product" />
                            <tns:global identifier="productadd_productitem" type="cn.ibizlab.crm.core.crm.domain.product" />
                            <tns:global identifier="productadd_productrelation" type="cn.ibizlab.crm.core.crm.domain.relation" />
            </extensionElements>
                        <startEvent id="begin" isInterrupting="true"/>
                        <businessRuleTask activiti:exclusive="true" g:ruleFlowGroup="productadd_productprepareparam1" id="prepareparam1" implementation="http://www.jboss.org/drools/rule" name="获取选择产品集合"/>
                        <businessRuleTask activiti:exclusive="true" g:ruleFlowGroup="productadd_productloopsubcall1" id="loopsubcall1" implementation="http://www.jboss.org/drools/rule" name="循环获取产品数据集"/>
                        <businessRuleTask activiti:exclusive="true" g:ruleFlowGroup="productadd_productcopyparam1" id="copyparam1" implementation="http://www.jboss.org/drools/rule" name="拷贝产品数据至中间对象"/>
                        <businessRuleTask activiti:exclusive="true" g:ruleFlowGroup="productadd_productend1" id="end1" implementation="http://www.jboss.org/drools/rule" name="结束"/>
                        <endEvent id="end1_end" name="end"/>
                        <sequenceFlow id="end1_end_line" sourceRef="end1" targetRef="end1_end"/>
                        <businessRuleTask activiti:exclusive="true" g:ruleFlowGroup="productadd_productprepareparam2" id="prepareparam2" implementation="http://www.jboss.org/drools/rule" name="设置关联数据"/>
                        <businessRuleTask activiti:exclusive="true" g:ruleFlowGroup="productadd_productdeaction1" id="deaction1" implementation="http://www.jboss.org/drools/rule" name="创建关联数据"/>
                        <endEvent id="deaction1_end" name="end"/>
                        <sequenceFlow id="deaction1_end_line" sourceRef="deaction1" targetRef="deaction1_end"/>
                    <sequenceFlow id="gateway-begin_prepareparam1" sourceRef="gateway-begin" targetRef="prepareparam1">
                            <conditionExpression  language="http://www.jboss.org/drools/rule" ><![CDATA[
                                            $productadd_productdefault:product();
                                            $productadd_productitem:product();
                                            $productadd_productrelation:relation();
                                            $productadd_productsrfactionparam:Map();
                                            $productadd_productsub:Map();
                                    eval($productadd_productdefault==productadd_productdefault && $productadd_productitem==productadd_productitem && $productadd_productrelation==productadd_productrelation && );
                                eval((RuleUtils.test($productadd_productdefault.get("srfactionparam"),"ISNOTNULL","")))
                                ]]>
                            </conditionExpression>
                    </sequenceFlow>
                    <sequenceFlow id="prepareparam1_loopsubcall1" sourceRef="prepareparam1" targetRef="loopsubcall1">
                    </sequenceFlow>
                    <sequenceFlow id="prepareparam1_end1" sourceRef="prepareparam1" targetRef="end1">
                    </sequenceFlow>
                    <sequenceFlow id="loopsubcall1_copyparam1" sourceRef="loopsubcall1" targetRef="copyparam1">
                    </sequenceFlow>
                    <sequenceFlow id="copyparam1_prepareparam2" sourceRef="copyparam1" targetRef="prepareparam2">
                    </sequenceFlow>
                    <sequenceFlow id="prepareparam2_deaction1" sourceRef="prepareparam2" targetRef="deaction1">
                    </sequenceFlow>
                            <exclusiveGateway id="gateway-begin" name="Gateway" gatewayDirection="Diverging" default="gateway-begin_end_line"></exclusiveGateway>
                            <sequenceFlow id="begin_gateway-begin_gatewayline" sourceRef="begin" targetRef="gateway-begin"></sequenceFlow>
                            <sequenceFlow id="gateway-begin_end_line" sourceRef="gateway-begin" targetRef="gateway-begin_end"/>
                            <endEvent id="gateway-begin_end" name="end"/>
        </process>
    </definitions>