    "codeName" : "calculate_amount",
    "name" : "更新分配金额",
    "scriptCode" : "var payee_allocates_info = data.payee_allocates;\r\nfor(var i=0; i<payee_allocates_info.length; i++){\r\n    var payee_allocate = data.payee_allocates[i];\r\n    var amount = payee_allocate.amount;\r\n    var ratio = payee_allocate.ratio;\r\n    if (typeof amount !== 'undefined' && amount !== null && amount !== ''){\r\n\r\n    } else {        \r\n        amount =  data.amount_payee * ratio * 0.01;\r\n        amount = parseFloat(amount.toFixed(2));\r\n        var amountString = amount.toString();\r\n        if (amountString.indexOf('.') !== -1 && amountString.split('.')[1].length === 1) {\r\n            amountString += '0';\r\n        }\r\n        payee_allocate.amount = amountString;\r\n        data.payee_allocates[i] = payee_allocate;\r\n        }\r\n}",
